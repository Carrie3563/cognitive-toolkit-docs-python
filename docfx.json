{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "cntk2.0/docs-ref-conceptual",
        "dest": "cntk",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "cntk2.0/docs-ref-autogen",
        "dest": "api",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {},
    "fileMetadata": {},
    "template": [],
    "dest": "cntk"
  }
}
