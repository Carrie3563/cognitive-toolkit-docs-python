{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-conceptual/2.0",
        "version": "cntk-2.0",
        "dest": "cognitive-toolkit",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "docs-ref-autogen/2.0",
        "version": "cntk-2.0",
        "dest": "api",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "breadcrumb",
        "dest": "breadcrumb",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "src": "docs-ref-conceptual/2.0",
        "version": "cntk-2.0",
        "dest": "cognitive-toolkit",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "versions": {
      "cntk-2.0": {
        "dest": "cntk-2.0"
      }
    },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/python/breadcrumb/toc.json"
    },
    "fileMetadata": {
      "author": {
        "docs-ref-autogen/2.0/**/*.yml": "VSC-Service-Account"
      },
      "ms.devlang": {
        "docs-ref-autogen/2.0/**/*.yml": "python"
      }
    },
    "template": [],
    "dest": "cntk"
  }
}
