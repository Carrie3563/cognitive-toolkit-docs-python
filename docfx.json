{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-conceptual/2.0",
        "version": "cntk-py-2.0",
        "dest": "cognitive-toolkit",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "docs-ref-autogen/2.0",
        "version": "cntk-py-2.0",
        "dest": "api",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.yml"
        ],
        "src": "breadcrumb",
        "dest": "breadcrumb",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "src": "docs-ref-conceptual/2.0",
        "version": "cntk-py-2.0",
        "dest": "cognitive-toolkit",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "versions": {
      "cntk-py-2.0": {
        "dest": "cntk-py-2.0"
      }
    },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/python/breadcrumb/toc.json",
      "searchScope": ["Cognitive Toolkit"]
    },
    "fileMetadata": {
      "author": {
        "docs-ref-autogen/2.0/**/*.yml": "mahilleb-msft"
      },
      "ms.author": {
        "docs-ref-autogen/2.0/**/*.yml": "mahilleb"
      },
      "manager": {
        "docs-ref-autogen/2.0/**/*.yml": "chazhang"
      },
      "ms.devlang": {
        "docs-ref-autogen/2.0/**/*.yml": "python",
        "docs-ref-conceptual/2.0/**/*.md": "python"
      },
      "ms.date": {
        "docs-ref-autogen/2.0/**/*.yml": "5/31/2017"
      },
      "ms.service": {
        "docs-ref-autogen/2.0/**/*.yml": "cognitive-toolkit"
      }
    },
    "template": [],
    "dest": "cntk"
  }
}
